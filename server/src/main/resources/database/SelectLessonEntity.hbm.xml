<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="database.SelectLessonEntity" table="SelectLesson" schema="dbo" catalog="dbPocketTest">
        <composite-id mapped="true" class="database.SelectLessonEntityPK">
            <key-property name="lessonId">
                <column name="LessonID" sql-type="varchar(20)" length="20"/>
            </key-property>
            <key-property name="term">
                <column name="Term" sql-type="char(6)" length="6"/>
            </key-property>
            <key-property name="stuId">
                <column name="StuID" sql-type="varchar(15)" length="15"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>