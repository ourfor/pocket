<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="database.AgentServerEntity" table="AgentServer" schema="dbo" catalog="dbPocketTest">
        <id name="SvrId">
            <column name="SvrID" sql-type="smallint"/>
        </id>
        <property name="SvrCode">
            <column name="SvrCode" sql-type="char(16)" length="16" not-null="true"/>
        </property>
        <property name="Version">
            <column name="Version" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="SvrKey">
            <column name="SvrKey" sql-type="uniqueidentifier" not-null="true"/>
        </property>
        <property name="RoomId">
            <column name="RoomID" sql-type="smallint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>